{
    "name": "moham7dreza/bazaar-laravel",
    "type": "project",
    "description": "advertise shopping site",
    "homepage": "https://github.com/moham7dreza/bazaar-laravel",
    "keywords": [
        "laravel",
        "framework"
    ],
    "authors": [
        {
            "name": "Mohamadreza Rezaei",
            "email": "me.moham6dreza@gmail.com"
        }
    ],
    "version": "1.0.0",
    "support": {
        "email": "me.moham6dreza@gmail.com"
    },
    "readme": "./README.md",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-mongodb": "*",
        "ext-pcntl": "*",
        "amiriun/sms": "dev-master",
        "cviebrock/eloquent-sluggable": "^12.0",
        "dedoc/scramble": "^0.12",
        "doctrine/dbal": "^4.2",
        "dyrynda/laravel-cascade-soft-deletes": "^4.0",
        "internachi/modular": "^2.3",
        "intervention/image-laravel": "^1.5",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.31",
        "laravel/octane": "^2.8",
        "laravel/pulse": "^1.4",
        "laravel/reverb": "^1.5",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.6",
        "laravel/tinker": "^2.0",
        "modules/advertise": "*",
        "modules/auth": "*",
        "modules/content": "*",
        "modules/filament": "*",
        "mongodb/laravel-mongodb": "^5.4",
        "morilog/jalali": "^3.4",
        "munafio/chatify": "^1.6",
        "opcodesio/log-viewer": "^3.15",
        "shetabit/payment": "^6.1",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-health": "^1.33",
        "spatie/laravel-prometheus": "^1.2",
        "spatie/laravel-schedule-monitor": "^3.10",
        "spatie/security-advisories-health-check": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.23",
        "imanghafoori/laravel-microscope": "^1.0",
        "larastan/larastan": "^3.0",
        "laravel/breeze": "^2.0",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.21",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "pestphp/pest-plugin-type-coverage": "^3.5",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.4",
        "spatie/laravel-web-tinker": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/commons.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:assets"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('.env.testing') || copy('.env.testing.example', '.env.testing');\"",
            "make githooks"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi --force --seed",
            "@php artisan storage:link"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
