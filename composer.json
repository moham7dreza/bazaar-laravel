{
    "name": "moham7dreza/bazaar-laravel",
    "type": "project",
    "description": "advertise shopping site",
    "homepage": "https://github.com/moham7dreza/bazaar-laravel",
    "keywords": ["laravel", "framework"],
    "authors": [
        {
            "name": "Mohamadreza Rezaei",
            "email": "me.moham6dreza@gmail.com"
        }
    ],
    "version": "1.0.0",
    "support": {
        "email": "me.moham6dreza@gmail.com"
    },
    "readme": "./README.md",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-mongodb": "*",
        "ext-pcntl": "*",
        "amiriun/sms": "dev-master",
        "cviebrock/eloquent-sluggable": "^12.0",
        "dedoc/scramble": "^0.12",
        "doctrine/dbal": "^4.2",
        "dyrynda/laravel-cascade-soft-deletes": "^4.0",
        "filament/filament": "3.3",
        "hasnayeen/themes": "^3.0",
        "internachi/modular": "^2.3",
        "intervention/image": "^3.7",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.31",
        "laravel/pulse": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.6",
        "laravel/tinker": "^2.0",
        "modules/demo": "*",
        "mongodb/laravel-mongodb": "^5.2",
        "morilog/jalali": "^3.4",
        "opcodesio/log-viewer": "^3.15",
        "shetabit/payment": "^6.1",
        "shuvroroy/filament-spatie-laravel-backup": "^2.2",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-health": "^1.33",
        "spatie/laravel-permission": "^6.16",
        "spatie/laravel-prometheus": "^1.2",
        "spatie/security-advisories-health-check": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.21",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-drift": "^3.0",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "pestphp/pest-plugin-type-coverage": "^3.5",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/commons.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan modules:sync",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:eloquent"
        ],
        "reload": [
            "@php artisan clear-compiled",
            "composer install --ignore-platform-reqs",
            "@php artisan optimize:clear",
            "@php artisan modules:clear",
            "@php artisan filament:optimize-clear",
            "@php artisan migrate --force"
        ],
        "testp": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan migrate --force --env=testing",
            "@php artisan test -p"
        ],
        "testpf": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan test -p --recreate-databases"
        ],
        "cache": [
            "@php artisan optimize",
            "@php artisan modules:cache",
            "@php artisan filament:optimize"
        ],
        "dev": [
            "git pull",
            "composer reload",
            "composer ide-helper",
            "@php artisan queue:work"
        ],
        "prod": [
            "git pull",
            "@php artisan clear-compiled",
            "composer install --ignore-platform-reqs --optimize-autoloader --no-dev",
            "composer run-script post-install",
            "composer dump-autoload --optimize",
            "@php artisan optimize:clear",
            "@php artisan modules:clear",
            "@php artisan migrate --force",
            "composer cache",
            "@php artisan queue:work"
        ],
        "setup": [
            "git pull",
            "@php artisan key:generate",
            "npm run setup",
            "composer dev"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
